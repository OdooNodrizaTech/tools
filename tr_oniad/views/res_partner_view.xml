<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="tr_oniad_view_partner_form" model="ir.ui.view">
        <field name="name">tr_oniad_view_partner_form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <page name="accounting" position="after">
                <page string="Tracking" name="tracking">
                    <group>
                        <field name="tracking_profile_uuid" />
                        <field name="tracking_cookie_uuid" /> 
                        <field name="tracking_user_uuid" />
                        <field name="tracking_session_uuid" />
                    </group>                   
                </page>
            </page>
        </field>
    </record>

</odoo>
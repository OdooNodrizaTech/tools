            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model" eval="'phone.call.log.file'" />
            <field name="function" eval="'cron_phone_call_log_files'" />
            <field name="args" eval="" />
        </record>
    </data>
</odoo>